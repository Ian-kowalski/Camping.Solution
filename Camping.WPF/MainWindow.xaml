<Window x:Class="Camping.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Camping.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Margin="24,23,0,377" Content="Reserveren" HorizontalAlignment="Left" Width="168" Click="Reserveren_click"/>
        <Button Margin="232,23,0,377" Content="Reserveringen" HorizontalAlignment="Left" Width="168" Click="Reserveringen_click"/>
        <ListBox x:Name="CampingList" Margin="0,0,0,26" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Center" Width="752" Height="320" VerticalAlignment="Bottom">
            <ListBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="0.094"/>
                    <RotateTransform/>
                    <TranslateTransform Y="0.58"/>
                </TransformGroup>
            </ListBox.RenderTransform>
        </ListBox>
    </Grid>
</Window>
